<h1>User Sign up </h1>
<%= form_with url: post_user_details_user_index_path, method: :post, local: true do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email, required: true %>
  <div></div>
  <%= form.label :password %>
  <%= form.password_field :password, required: true %>
  <%= form.submit 'Create Account' %>
<% end %>

